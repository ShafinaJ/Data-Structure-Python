MAX_SIZE=5
Stack=[]
top=-1
def push (book_title):
    global top
    if top>=MAX_SIZE-1:
        print("Stack overflow cannot push more books:")
    else:
        top+=1
        Stack.append(book_title)
        print(f"Book '{book_title}'pushed onto the book:")
def pop():
    global top
    if top==-1:
        print("Stack underflow ! cannot pop my stack")
    else:
        removed_book=Stack.pop()
        print(f"Book '{removed_book}'poppped from the stack")
        top-=1
def peek():
    if top==-1:
        print("Stack is empty,no book is peek")
    else:
        print(f"Top book on the stack:'{Stack[top]}'")
def display():
    if top==-1:
        print("SStack is empty")
    else:
        print("Book is stack (top to bottom)")
        for i in range (top,-1,-1):
            print(f"{i+1}){Stack[i]}")
push("Harry Potter")
push("The Alchemist")
push("The Curious")
push("The Hill WE Climb")
push("Dear Self")
push("Exra Book")
display()
peek()
pop()
pop()
display()
peek()

            
        
        
